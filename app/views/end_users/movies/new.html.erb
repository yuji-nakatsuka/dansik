<h2>New Post</h2>

<%= form_for(@movie) do |f| %>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title,autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </div>


  <div class="actions">
    <%= f.submit "Create" %>
  </div>
<% end %>
